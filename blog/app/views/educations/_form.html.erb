<%= form_for([@resume, @resume.skills.build]) do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :time %><br>
    <%= f.text_field :time %>
  </p>
  <p>
    <%= f.label :degree %><br>
    <%= f.text_field :degree %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>